@{
}
@model List<GameKeeper.Web.Models.Entities.GameRecord>

<h1>Players</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Game</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var player in Model)
        {
            <tr>
                <td>@player.Id</td>
                <td>@player.Game</td>
                <td>
                    <a>Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>